{
  "name": "upload-testflight-action",
  "version": "3.0.0",
  "private": true,
  "description": "A GitHub Action that uploads a app to Apple's TestFlight",
  "main": "lib/main.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc --outDir lib",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "lint": "eslint",
    "package": "ncc build --license licenses.txt",
    "all": "yarn format && yarn lint && yarn build && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apple-Actions/upload-testflight-build.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Itty Bitty Apps Pty Ltd",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@typescript-eslint/parser": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.0.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
